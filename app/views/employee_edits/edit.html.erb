<%= render "common/panel", title: 'Edit Employee' do %>


  <%= form_tag("/employee_edits/#{@employee.id}", method: :put) do %>
    <%= render 'form', employee: @employee %>
  <% end %>

  <br/>

  <div class="row">

    <div class="col-md-2">
      <%= link_to 'Show', employee_edit_path(@employee), class: "btn btn-primary" %>
    </div>

    <div class="col-md-2">
      <%= link_to 'Back', employee_edits_path, class: "btn btn-primary" %>
    </div>
  </div>

<% end %>
